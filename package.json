  "description": "The nodejs api library for accessing coingecko api v3 , develop with typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --fix",
    "build": "rm -rf ./dist && tsc",
    "dev": "ts-node ./src/index",
